fun print_square(w: i32, h: i32) >> void {
    i32 i = 0;
    i32 j = 0;
    for {
        for {
            j = j + 1;
            @printf("#");
            if j >= w {break;}
        }
        j = 0;
        i = i + 1;
        @printf("\n");
        if i >= h {break;}
    }
}

fun main() >> i32 {
    i32 i = 1;
    for {
        print_square(i * 2, i);
        @printf("\n\n\n");
        i = i + 1;
        if i > 5 {break;} ?? check with return
    }
    return 0;
}