fun complexTest(n: i32) >> i32 {
    if n > 100 {
        return n * 2;
    } else if n > 50 {
        return n;
    } else if n > 0 {
        if n > 25 {
            return n - 1;
        } else {
            return n + 1;
        }
    } else if n == 0 {
        return 0;
    } else {
        return -1;
    }
}

fun main() >> i32 {
    i32 result1 = complexTest(150);
    i32 result2 = complexTest(75);
    i32 result3 = complexTest(30);
    i32 result4 = complexTest(10);
    i32 result5 = complexTest(0);
    i32 result6 = complexTest(-5);
    @printf("Results: %d, %d, %d, %d, %d, %d\n", result1, result2, result3, result4, result5, result6);
    return 0;
}