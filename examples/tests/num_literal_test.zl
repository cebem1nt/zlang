fun main() >> i32 {
    i32 simple  = 2;
    i32 neg     = -2;
    i32 pos     = +2;
    i32 exp_pos = 2e2;
    f32 exp_neg = 2e-2;
    f32 exp_zer = 2e0;
    f32 neg_exp = -2e-2;
    f32 neg_var = -neg_exp;
    
    @printf("%d\n", simple);
    @printf("%d\n", neg);
    @printf("%d\n", pos);
    @printf("%d\n", exp_pos);
    @printf("%f\n", exp_neg);
    @printf("%f\n", exp_zer);
    @printf("%f\n", neg_exp);
    @printf("%f\n", neg_var);
    
    return 0;
}