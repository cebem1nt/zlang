fun main() >> i32 {
    bool eq = 5 == 5;
    bool not_eq = true != false;
    bool fcomp = .5 < 1.0;
    bool scomp = -5 > -20;
    bool eqg = 1 >= 1;
    bool non = -500 != 200;
    bool and = true && true;
    bool or = true || false;
    bool complex = ((1&&0.5)||(false||true||false)) && ((false||.5>.0)&&(true&&500>0));
    @printf("(expected 111111111):%d%d%d%d%d%d%d%d%d\n", eq, not_eq, fcomp, scomp, eqg, non, and, or,complex);
    
    bool a = !true;
    bool b = !false;
    bool c = !(1 > 0);
    bool d = !0;
    
    @printf("(expected 0101):%d%d%d%d\n", a, b, c ,d);
    return 0;
}