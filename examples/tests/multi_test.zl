fun helper() >> void {
    @printf("Helper function called\n");
    return;
}

fun get_number() >> i32 {
    i32 value = 42;
    @printf("Returning number: %d\n", value);
    return value;
}

fun cleanup() >> void {
    @system("sleep 5 && clear");
    @printf("Cleanup complete!\n");
    return;
}

fun main() >> i32 {
    i32 x = 10;
    i32 y = 20;

    @printf("Starting main function\n");
    helper();

    i32 result = get_number();
    @printf("Got result: %d\n", result);
    cleanup();
    return 0;
}
