fun main() >> i32 {
    i32 a = 250;
    i32 b = 50;
    i32 sum = a + b;
    @printf("%d + %d = %d\n", a, b, sum);
    i32 dif = a - b;
    @printf("%d - %d = %d\n", a, b, dif);
    i32 mul = a * b;
    @printf("%d * %d = %d\n", a, b, mul);
    i32 div = a / b;
    @printf("%d / %d = %d\n", a, b, div);
    i32 br_ = (a+50) / b;
    @printf("(%d+50) / %d = %d\n", a, b, br_);
    a = a + a;
    @printf("250 +250 = %d\n", a);
    @printf("2 * 2 = %d", 2*2);
    f32 f1 = 3.14;
    f32 f2 = 14.88;
    f64 f3 = f1 * f2;
    @printf("%f + %f = %f\n", f1, f2, f1+f2);
    @printf("%f * %f = %f\n", f1, f2, f3);
    f16 f16_ = 1.2;
    f64 f64_ = 2.1;
    @printf("f64 + f16 test (should be 3.3): %.1f\n", f64_+f16_);
    f64_ = f64_ + f1 + f16_ + 0.0 + 1.1 + 2.222222222;
    return 0;
}